{
  "name": "arriva-tickets",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "./node_modules/.bin/eslint --quiet source jest App.tsx",
    "eject": "expo eject",
    "test": "jest --coverage",
    "tsc": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@notifee/react-native": "^7.6.1",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-community/netinfo": "^9.3.8",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@tanstack/query-async-storage-persister": "^4.20.4",
    "@tanstack/react-query": "^4.20.4",
    "@tanstack/react-query-persist-client": "^4.20.4",
    "axios": "^1.2.2",
    "expo": "^48.0.0",
    "expo-secure-store": "~12.1.1",
    "expo-status-bar": "~1.4.4",
    "luxon": "^3.2.1",
    "native-base": "^3.4.8",
    "react": "18.2.0",
    "react-native": "0.71.6",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "^13.9.0",
    "react-native-swipeable": "^0.6.0",
    "ts-invariant": "^0.10.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@callstack/eslint-config": "^13.0.2",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.0.1",
    "@types/jest": "^29.5.0",
    "@types/luxon": "^3.2.0",
    "@types/react": "~18.0.27",
    "@types/react-native": "^0.71.5",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.0.3",
    "nock": "^13.2.8",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write -u"
    ],
    "**/*.{json}": "eslint --fix"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!axios)/",
      "jest-runner"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest/setup.ts"
    ]
  },
  "private": false,
  "repository": "git@github.com:jbazant/arriva-tickets.git",
  "author": "Jiří Bažant <j.bazant@gmail.com>",
  "license": "MIT"
}
