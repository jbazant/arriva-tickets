    "expo": "~45.0.0",
    "expo-secure-store": "~11.2.0",
    "expo-status-bar": "~1.3.0",
    "mobx": "^6.6.1",
    "mobx-react-lite": "^3.4.0",
    "mobx-state-tree": "^5.1.5",
    "mst-persist": "^0.1.3",
